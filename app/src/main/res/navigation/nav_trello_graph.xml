<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_trello_graph"
    app:startDestination="@id/trelloPageFragment">

    <fragment
        android:id="@+id/trelloPageFragment"
        android:name="com.seda.trello.fragments.TrelloPageFragment"
        android:label="Trello"
        tools:layout="@layout/fragment_trello_page" >
        <action
            android:id="@+id/action_trelloPageFragment_to_createBoardActivity"
            app:destination="@id/createBoardActivity"
            app:popUpTo="@id/trelloPageFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <activity
        android:id="@+id/createBoardActivity"
        android:name="com.seda.trello.fragments.CreateBoardActivity"
        android:label="activity_create_board"
        tools:layout="@layout/activity_create_board" />
</navigation>